{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "ordered-imports": true,
    "quotemark": [true, "single"],
    "no-string-literal": false
  },
  "rulesDirectory": []
}
